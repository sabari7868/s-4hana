<mvc:View
  controllerName="inventory.app.controller.Inventory"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m">
  <Page title="Warehouse Inventory">
    <Table id="inventoryTable" items="{/A_MaterialStock}">
      <columns>
        <Column><Text text="Material"/></Column>
        <Column><Text text="Stock"/></Column>
      </columns>
      <items>
        <ColumnListItem>
          <cells>
            <Text text="{Material}"/>
            <Text text="{Stock}"/>
          </cells>
        </ColumnListItem>
      </items>
    </Table>
  </Page>
</mvc:View>
