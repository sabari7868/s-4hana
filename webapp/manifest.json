
{
  "sap.app": {
    "id": "inventory.app",
    "type": "application",
    "title": "Warehouse Inventory Tracker",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "inventory.app.view.Inventory",
      "type": "XML",
      "id": "mainView"
    },
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.model.odata.v2": {}
      }
    },
    "models": {
      "": {
        "dataSource": "MaterialStock",
        "preload": true
      }
    },
    "dataSources": {
      "MaterialStock": {
        "uri": "/SAP_MATERIAL_STOCK_API/sap/opu/odata/sap/API_MATERIAL_STOCK_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    },
    "resources": {
      "js": [],
      "css": []
    }
  }
}
